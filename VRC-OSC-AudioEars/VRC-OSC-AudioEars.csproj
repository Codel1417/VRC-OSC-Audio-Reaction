<?xml version="1.0" encoding="utf-8"?>


<Project Sdk="Microsoft.NET.Sdk">


<PropertyGroup>
        <OutputType>WinExe</OutputType>
        <RootNamespace>VRC_OSC_AudioEars</RootNamespace>
        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>VRC OSC Audio Reaction</Title>
        <Description>This take the audio from your default audio output device and sends OSC parameters for direction and amplitude.</Description>
        <PackageProjectUrl>https://github.com/Codel1417/VRC-OSC-Audio-Reaction</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/Codel1417/VRC-OSC-Audio-Reaction/blob/master/license.txt</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/Codel1417/VRC-OSC-Audio-Reaction</RepositoryUrl>
        <NeutralLanguage>en-US</NeutralLanguage>
        <IsPackable>false</IsPackable>
        <RepositoryType>git</RepositoryType>
        <PlatformTarget>x64</PlatformTarget>
        <BaseOutputPath>bin\</BaseOutputPath>
        <RuntimeIdentifier>win10-x64</RuntimeIdentifier>
        <PublishSingleFile>true</PublishSingleFile>
        <SelfContained>false</SelfContained>
        <PublishTrimmed>false</PublishTrimmed>
        <PublishReadyToRun>true</PublishReadyToRun>
        <PublishProfile>Release</PublishProfile>
	    <DebugType>embedded</DebugType>
        <BuiltInComInteropSupport>true</BuiltInComInteropSupport>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <FileVersion>$(AssemblyVersion)</FileVersion>
        <InformationalVersion>$(AssemblyVersion)</InformationalVersion>
        <ProductVersion>$(AssemblyVersion)</ProductVersion>
        <Company>Codel1417</Company>
        <AssemblyName>VRC_OSC_AudioEars</AssemblyName>
        <ProjectTypeGuids>{60dc8134-eba5-43b8-bcc9-bb4bc16c2548};{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</ProjectTypeGuids>
	    <TargetFramework>net6.0-windows10.0.22000.0</TargetFramework>
        <UseWPF>true</UseWPF>
        <LangVersion>10</LangVersion>
	    <WarningLevel>4</WarningLevel>
	    <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
	    <PublishReadyToRunComposite>true</PublishReadyToRunComposite>
	    <SignAssembly>False</SignAssembly>
</PropertyGroup>
    <ItemGroup>
        <PackageReference Include="JetBrains.Annotations" Version="2022.1.0" />
        <PackageReference Include="MaterialDesignColors" Version="2.0.6" />
        <PackageReference Include="MaterialDesignThemes" Version="4.5.0" />
        <PackageReference Include="MaterialDesignThemes.Wpf" Version="1.0.1" />
        <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
        <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
        <PackageReference Include="Microsoft.Win32.SystemEvents" Version="6.0.1" />
        <PackageReference Include="NAudio.Wasapi" Version="2.1.0" />
        <PackageReference Include="NLog" Version="5.0.0" />
        <PackageReference Include="NLog.DiagnosticSource" Version="1.2.0" />
        <PackageReference Include="Octokit" Version="0.51.0" />
        <PackageReference Include="Sentry" Version="3.17.1" />
        <PackageReference Include="Sentry.DiagnosticSource" Version="3.19.0" />
        <PackageReference Include="Sentry.NLog" Version="3.17.1" />
        <PackageReference Include="System.Collections" Version="4.3.0" />
        <PackageReference Include="VRCOscLib" Version="1.1.3" />
    </ItemGroup>
    <ItemGroup>
      <Compile Update="Settings.Designer.cs">
        <DesignTimeSharedInput>True</DesignTimeSharedInput>
        <AutoGen>True</AutoGen>
        <DependentUpon>Settings.settings</DependentUpon>
      </Compile>
    </ItemGroup>
    <ItemGroup>
      <None Update="Settings.settings">
        <Generator>SettingsSingleFileGenerator</Generator>
        <LastGenOutput>Settings.Designer.cs</LastGenOutput>
      </None>
    </ItemGroup>
</Project>
